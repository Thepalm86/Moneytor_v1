# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=your_supabase_project_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key

# Supabase Service Role (Server-side only - DO NOT expose to client)
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# Supabase CLI Access Token (for local development)
SUPABASE_ACCESS_TOKEN=your_supabase_access_token

# Application URLs (Required for security)
NEXT_PUBLIC_SITE_URL=http://localhost:3000

# GitHub Repository Information
GIT_USER_EMAIL=your_git_email
GIT_USERNAME=your_git_username
GIT_REPOSITORY=your_repository_name

# Settings Redesign Feature Flags
NEXT_PUBLIC_ENABLE_SETTINGS_REDESIGN=false
NEXT_PUBLIC_SETTINGS_REDESIGN_ROLLOUT=0

# Advanced Settings Features
NEXT_PUBLIC_ENABLE_ADVANCED_SEARCH=false
NEXT_PUBLIC_ENABLE_QUICK_ACTIONS=false